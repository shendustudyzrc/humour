<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
 "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
 <!-- 映射 -->
 <mapper namespace="com.mybatis.ManageMapper">
 <!-- 根据id号进行查找 -->
 <select id="selectManageLogin" parameterType="Manage" resultType="Manage">
   select name,password from manage where name=#{name} and password=#{password};
 </select>
 </mapper>